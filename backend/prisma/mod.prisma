model Mod {
  mod_id    Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  phone     String   @unique
  password  String
  verified  Boolean  @default(false)
  create_at DateTime @default(now())
  update_at DateTime @updatedAt
  store_id  String
  store     Store    @relation(fields: [store_id], references: [store_id], onDelete: Cascade)
}